{
    "name": "tbcd/webshop-services",
    "type": "symfony-bundle",
    "license": "MIT",
    "version": "0.0.1",
    "description": "A skeleton to start a new Symfony website",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-client": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^6.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "TBCD\\Webshop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.4"
        }
    }
}
